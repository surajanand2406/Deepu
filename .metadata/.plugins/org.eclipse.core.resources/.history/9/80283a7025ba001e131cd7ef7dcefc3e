import java.util.*;

public class Solution {
	
	private Boolean isSpecialBinaryTree(TreeNode root) {
		if(root.data == null) {
			return false;
		}
		if(root.data != null) {
			TreeNode odd = root.left%2;
			TreeNode even = root.rigth%2;
			if(root.data == 1 && temp != 0 && even == 0) {
				return true;
			}
		}
	
	return false;
}

}
